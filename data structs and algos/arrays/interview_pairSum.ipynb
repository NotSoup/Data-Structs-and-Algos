   "source": [
    "def pair_sum(arr, target):\n",
    "    pairs = 0\n",
    "    hashmap = {}\n",
    "    for i in arr:\n",
    "        if i in hashmap:\n",
    "            pairs += 1\n",
    "        hashmap[target - i] = i\n",
    "    return pairs"
   ]
  },
